package ar.edu.unlp.info.oo2.FileManager;
import java.util.*;

public class FileManager {
	
	private List<File> files;
	
	public FileManager() {
		files = new ArrayList<File>();
	}
	
	public String prettyPrint(File aFile) {
		if (files.contains(aFile)){
			return aFile.prettyPrint();
		}
		else return null;
	}
	
	
}
